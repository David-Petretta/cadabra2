
# Tests for the connection to a Mathematica kernel.

def test01():
    ex:= \alpha \beta \cos{\gamma \Delta}:
    out=ex.mma_form()
    tst="\[Alpha]*\[Beta]*Cos[\[Gamma]*\[CapitalDelta]]"
    assert(out==tst)
    print("Test 01 passed")

test01()

